<h1>日報一覧</h1>
<% @reports.each.with_index(1) do |report, i| %>
  <p>
  日報ID：<%= report.id %>
  </p>
  <p>日付：<%= report.created_on %></p>
  <br>
  <% report.report_items.each do |item| %>
    <p>
    ID：<%= item.id %>
    ジャンル：<%= item.genre_id %>
    やったこと：<%= item.content %>
    時間：<%= item.work_hours %>
    </p>
  <% end %>
  コメント： <%= report.content %>
  </p>
  <hr>
<% end %>